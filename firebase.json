{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**/*.@(jpg|jpeg|gif|png|woff)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=604800"
      } ]
    } ]
  }
}
